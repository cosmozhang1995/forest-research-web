<form id="form" action="/result" method="post" role="form" enctype="multipart/form-data">
	<div class="form-group">
		<!-- <label>上传你的表格，一定要是 .xlsx 格式的哦</label> -->
		<input type="file" id="file" name="file" style="display:none;" />
		<input type="text" id="category" name="category" style="display:none;" />
		<!-- <div id="file-dropbox" class="file-dropbox">
			<div class="file-dropbox-content">上传你的表格，一定要是 .xlsx 格式的哦</div>
		</div> -->
		<input type="text" id="single_feature" name="single_feature" value="false" style="display:none;" />
		<div class="form-group">
			<div class="btn-group">
				<button type="button" class="btn btn-file-choose btn-success">上传你的表格，一定要是 .xlsx 格式的哦</button>
			</div>
		</div>
		<div class="form-group">
			<div class="btn-group">
				<button type="button" class="btn btn-category-select btn-default" data-value="tree">乔木</button>
				<button type="button" class="btn btn-category-select btn-default" data-value="shrub">灌木</button>
				<button type="button" class="btn btn-category-select btn-default" data-value="herb">草本</button>
			</div>
		</div>
		<div class="form-group">
			<div class="btn-group">
				<button type="button" id="btn-check-single-feature" class="btn btn-check btn-default" data-field="single_feature" data-text="胸高断面积/冠幅/盖度 将乘以数目|忽略数目">忽略数目</button>
			</div>
		</div>
		<div class="form-group">
			<div class="btn-group">
				<button type="submit" class="btn btn-primary">开始分析</button>
			</div>
		</div>
	</div>
</form>
<script type="text/javascript" src="/javascripts/pages/page-index.js"></script>